public class me {
    public static void main(String[] args) throws Exception {

        student me = new student();
        me.surname = "Cruz";
        me.firstname = "Tyrel";
        me.middleInitial = 'P';
        me.dateOfBirth = "02/27/2003";
        me.studentNumber = 2022104871;
        me.studentEmailAddress = "cruztp@students.national-u.edu.ph";
        me.iAmAwesome = true;

        me.sayMysurname();
        me.sayMyfirstname();
        me.sayMymiddleInitial();
        me.sayMydateOfBirth();
        me.sayMystudentNumber();
        me.sayMystudentEmailAddress();
        me.sayiAmAwesome();
    }
}
